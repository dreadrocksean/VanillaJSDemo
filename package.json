{
  "name": "reusabletest",
  "version": "1.0.0",
  "description": "A pure javascript solution to reusable components complete with routing and menu list navigation.",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "build:sass": "node-sass -r --source-map-embed --source-map-contents --output-style compressed src/styles -o lib/styles",
    "watch:sass": "npm run build:sass && npm run build:sass -- -w",
    "start": "npm run watch:sass && babel src/js -w --minified -s true -d lib/js"
