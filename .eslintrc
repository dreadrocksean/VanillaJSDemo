{
	"env": {
		"es6": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-console": ["error", { "allow": ["warn", "error"] }],
		"quotes": [1, "single"],
		"semi": [1, "always"],
		"eqeqeq": "error"
	}
}